; /* Start:"a:4:{s:4:"full";s:50:"/local/templates/main/js/custom.js?170842746412376";s:6:"source";s:34:"/local/templates/main/js/custom.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
var notification = new bootstrap.Modal(document.getElementById('notification'))
function fNotification(title = "", text = "") {
  $('.modal').modal('hide');
  let modal = document.getElementById('notification');
  modal.querySelector('.modal-title').innerHTML = title;
  modal.querySelector('.modal-body').innerHTML = text;
  notification.show();
}

document.addEventListener('DOMContentLoaded', function () {

  var elements = document.querySelectorAll('.cust-header-search');
  elements.forEach(function (element) {
    element.addEventListener('mouseover', function () {
      this.classList.add('active');
    });
    element.addEventListener('mouseout', function () {
      var self = this;
      setTimeout(function () {
        self.classList.remove('active');
      }, 1000);
    });
  });

  var elements2 = document.querySelectorAll('.cust-nav-item-user');
  elements2.forEach(function (element) {
    element.addEventListener('mouseover', function () {
      this.classList.add('active');
    });
    element.addEventListener('mouseout', function () {
      var self = this;
      setTimeout(function () {
        self.classList.remove('active');
      }, 1000);
    });
  });


  //       
  if (window.location.pathname.indexOf("/search/") !== -1) {
    var element = document.querySelector('.product-page');
    if (element) {
      element.classList.add('product-page--search');
    }
  }

  // 
  const formAuth = document.querySelector('#authorization')
  formAuth.addEventListener('submit', (e) => {
    e.preventDefault();
    let formData = new FormData(formAuth);

    if (formData.get("USER_LOGIN") == "" || formData.get("USER_PASSWORD") == "") {
      formAuth.querySelector('.js-error').innerHTML = "   ";
    } else {
      formAuth.querySelector('.js-error').innerHTML = "";
      BX.ajax({
        url: '/local/ajax/authorization.php',
        data: formData,
        method: 'POST',
        dataType: 'json',
        processData: false,
        preparePost: false,
        onsuccess: function (result) {
          result = JSON.parse(result);
          // console.log(result);
          if (result.status) {
            document.location.href = "/personal/private/";
          } else {
            formAuth.querySelector('.js-error').innerHTML = result.message;
          }
        },
        onfailure: function () {
          formAuth.querySelector('.js-error').innerHTML = "  ";
        }
      });
    }
  });

  //   
  const formSendPassword = document.querySelector('#sendPassword')
  formSendPassword.addEventListener('submit', (e) => {
    e.preventDefault();
    let formData = new FormData(formSendPassword);

    if (formData.get("email") == "") {
      formSendPassword.querySelector('.js-error').innerHTML = "   ";
    } else {
      formSendPassword.querySelector('.js-error').innerHTML = "";
      BX.ajax({
        url: '/local/ajax/sendPassword.php',
        data: formData,
        method: 'POST',
        dataType: 'json',
        processData: false,
        preparePost: false,
        onsuccess: function (result) {
          result = JSON.parse(result);
          // console.log(result);
          if (result.status) {
            fNotification("         ");
          } else {
            formSendPassword.querySelector('.js-error').innerHTML = result.message;
          }
        },
        onfailure: function () {
          formSendPassword.querySelector('.js-error').innerHTML = "  ";
        }
      });
    }
  });

  // 
  const forms = document.querySelectorAll('form.js_form_standard');
  forms.forEach(form => {
    form.addEventListener('submit', (e) => {
      e.preventDefault();
      let formData = new FormData(form);

      if (formData.get("name") == "" || formData.get("phone") == "" || formData.get("consent") != "on") {
        alert("   ");
      } else if (formData.get("phone").replace(/[\D]+/g, '').length != 11) {
        alert("   ");
      } else {
        BX.ajax({
          url: '/local/ajax/form.standard.php',
          data: formData,
          method: 'POST',
          dataType: 'json',
          processData: false,
          preparePost: false,
          onsuccess: function (result) {
            console.log(result);
            result = JSON.parse(result);
            console.log(result);
            if (result.status == "success") {
              form.reset();
              fNotification("  ");
            } else {
              alert(result.text);
            }
          },
          onfailure: function (result) {
            console.log(result);
            alert("  ");
          }
        });
      }
    });
  });

  //   
  if (typeof favoritesProduct !== "undefined" && typeof favoritesOffers !== "undefined") {
    const btnFavorites = document.querySelectorAll(".js-favorites");

    if (btnFavorites.length > 0) {
      btnFavorites.forEach((btn) => {
        if (btn.dataset.type != undefined && btn.dataset.id != undefined) {
          const innerElement = btn.querySelector("i");

          //    
          if (btn.dataset.type == "product" && favoritesProduct.includes(Number(btn.dataset.id))) {
            btn.classList.add("active");
            innerElement.className = "flaticon-like";
          } else if (btn.dataset.type == "offer" && favoritesOffers.includes(Number(btn.dataset.id))) {
            btn.classList.add("active");
            innerElement.className = "flaticon-like";
          }

          btn.addEventListener("click", function (event) {
            event.preventDefault();
            if (userId > 0) {
              let data = {};
              data["type"] = btn.dataset.type;
              data["id"] = btn.dataset.id;
              if (btn.classList.contains("active")) {
                data["action"] = "remove";
              } else {
                data["action"] = "add";
                if (btn.dataset.type == "offer") {
                  data["product"] = btn.dataset.product;
                }
              }

              // console.log(data);

              //   
              if (window.BX && window.BX.ajax) {
                window.BX.ajax({
                  url: '/local/ajax/favorites.php',
                  method: 'POST',
                  data: data,
                  onsuccess: function (res) {
                    res = JSON.parse(res)
                    // console.log(res);

                    if (res['status']) {
                      if (btn.classList.contains("active")) {
                        btn.classList.remove("active");
                        innerElement.className = "flaticon-heart";
                      } else {
                        btn.classList.add("active");
                        innerElement.className = "flaticon-like";
                      }
                    } else {
                      alert(res['message']);
                    }
                  },
                  onfailure: function () {
                    alert(res['message']);
                  }
                })
              }
            } else {
              if (btn.classList.contains("active")) {
                if (btn.dataset.type == "offer") {
                  favoritesOffers = favoritesOffers.filter(function (f) {
                    return f !== Number(btn.dataset.id)
                  })
                } else {
                  favoritesProduct = favoritesProduct.filter(function (f) {
                    return f !== Number(btn.dataset.id)
                  })
                }
                btn.classList.remove("active");
                innerElement.className = "flaticon-heart";
              } else {
                if (btn.dataset.type == "offer") {
                  favoritesProduct.push(Number(btn.dataset.product))
                  favoritesOffers.push(Number(btn.dataset.id))
                } else {
                  favoritesProduct.push(Number(btn.dataset.id))
                }
                btn.classList.add("active");
                innerElement.className = "flaticon-like";
              }
              let date = new Date(Date.now());
              date.setFullYear(date.getFullYear() + 1);
              // console.log(date);
              date = date.toUTCString();

              document.cookie = "UF_FAVORITES_PRODUCT=" + JSON.stringify(favoritesProduct) + "; path=/; expires=" + date;
              document.cookie = "UF_FAVORITES_OFFERS=" + JSON.stringify(favoritesOffers) + "; path=/; expires=" + date;
            }

          });
        }
      });
    }
  }

  //  
  var customSelects = document.querySelectorAll(".custom-select");

  customSelects.forEach(function (select) {
    var selected = select.querySelector(".custom-select__selected");
    var options = select.querySelector(".custom-select__options");

    selected.addEventListener("click", function () {
      select.classList.toggle("open");
    });

    var selectOptions = options.querySelectorAll(".custom-select__option");
    selectOptions.forEach(function (option) {
      option.addEventListener("click", function () {
        var value = this.getAttribute("data-value");
        selected.textContent = this.textContent;
        select.classList.remove("open");
        var input = select.querySelector("input[type=hidden]");
        input.value = value;
        input.dispatchEvent(new Event('input'));
      });
    });

    var defaultValue = select.getAttribute("data-default");
    if (defaultValue) {
      // selected.textContent = defaultValue;
      var defaultOption = options.querySelector('.custom-select__option[data-value="' + defaultValue + '"]');
      if (defaultOption) {
        var defaultOptionValue = defaultOption.getAttribute("data-value");
        var input = select.querySelector("input[type=hidden]");
        input.value = defaultOptionValue;
        selected.textContent = defaultOption.textContent;
      }
    }

    document.addEventListener("click", function (event) {
      if (!select.contains(event.target)) {
        select.classList.remove("open");
      }
    });
  });

  //   
  let inputElementSort = document.querySelector('input[name="selecting-sort-option"]');
  if (inputElementSort != null) {
    inputElementSort.addEventListener("input", function (e) {
      document.cookie = "elementSort=" + inputElementSort.value+"; path=/;";
      location.reload();
    })
  }
});

$(document).ready(function () {
  $('input[type="tel"]').mask('+7 (000) 000-00-00');
  $('input[type="tel"]').attr('placeholder', '+7 (___) ___-__-__');

  if (document.querySelector('.slider-service-announcements .swiper')) {
    var swiper = new Swiper(".slider-service-announcements .swiper", {
      slidesPerView: 'auto',
      loop: true,
      centeredSlides: true,
      spaceBetween: 24,
      grabCursor: true,
      pagination: {
        el: ".slider-service-announcements .swiper-pagination",
        clickable: true,
      },
      on: {
        init: function () {
          equalizeSlideHeights(this);
        },
        resize: function () {
          equalizeSlideHeights(this);
        },
      }
    });
  }

  function equalizeSlideHeights(swiper) {
    var maxHeight = 0;
    swiper.slides.forEach(function (slide) {
      var height = slide.offsetHeight;
      if (height > maxHeight) {
        maxHeight = height;
      }
    });

    swiper.slides.forEach(function (slide) {
      slide.style.height = maxHeight + 'px';
    });
  }

});
/* End */
