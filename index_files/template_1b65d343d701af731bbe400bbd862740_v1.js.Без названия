function includeFiles(url) {
  var script = document.createElement('script');
  script.src = url;
  document.getElementsByTagName('head')[0].appendChild(script);
};

//   
document.addEventListener("DOMContentLoaded", () => {

  //   
  includeFiles("/local/templates/main/js/parts/catalog-menu.js?v=3");
  //includeFiles("/local/templates/main/js/parts/yandex.map.js");
  includeFiles("/local/templates/main/js/parts/sliders-collection.js?v=2");
  includeFiles("/local/templates/main/js/parts/show-details.js?v=2");
  includeFiles("/local/templates/main/js/parts/input-number.js?v=1");
  // includeFiles("/local/templates/main/js/parts/custom-header-cart.js?v=7");
  // includeFiles("/local/templates/main/js/parts/like-item.js");
  includeFiles("/local/templates/main/js/parts/promocode-enter.js");
  includeFiles("/local/templates/main/js/parts/cust-under-price.js");

  //   
  if (window.innerWidth <= 1400) {
    //      .cust-header-call .custom-header-phone__link  .cust-header-search__link
    var callElements = document.querySelectorAll(".cust-header-call .custom-header-phone__link");
    var searchElements = document.querySelectorAll(".cust-header-search__link");

    var allElements = [...callElements, ...searchElements];

    //        
    allElements.forEach(function (element) {
      element.addEventListener("click", function (event) {
        //    
        event.preventDefault();

        //   'active'
        this.classList.toggle('active');

        //   'active'    
        allElements.forEach(function (otherElement) {
          if (otherElement !== element) {
            otherElement.classList.remove('active');
          }
        });
      });
    });

    //      .top-search-close
    var closeElements = document.querySelectorAll(".top-search-close");

    //        
    closeElements.forEach(function (element) {
      element.addEventListener("click", function (event) {
        //    
        event.preventDefault();

        //       .cust-header-call  .cust-header-search
        var parentCall = this.closest(".cust-header-call");
        var parentSearch = this.closest(".cust-header-search");

        //       .custom-header-phone__link  .cust-header-search__link
        var linkCall = parentCall ? parentCall.querySelector(".custom-header-phone__link") : null;
        var linkSearch = parentSearch ? parentSearch.querySelector(".cust-header-search__link") : null;

        //      'active'
        if (linkCall) linkCall.classList.remove('active');
        if (linkSearch) linkSearch.classList.remove('active');
      });
    });
  }

});
/* End */
